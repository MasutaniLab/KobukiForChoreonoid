# KobukiBridge CMake config file
#
# This file sets the following variables:
# KobukiBridge_FOUND - Always TRUE.
# KobukiBridge_INCLUDE_DIRS - Directories containing the KobukiBridge include files.
# KobukiBridge_IDL_DIRS - Directories containing the KobukiBridge IDL files.
# KobukiBridge_LIBRARIES - Libraries needed to use KobukiBridge.
# KobukiBridge_DEFINITIONS - Compiler flags for KobukiBridge.
# KobukiBridge_VERSION - The version of KobukiBridge found.
# KobukiBridge_VERSION_MAJOR - The major version of KobukiBridge found.
# KobukiBridge_VERSION_MINOR - The minor version of KobukiBridge found.
# KobukiBridge_VERSION_REVISION - The revision version of KobukiBridge found.
# KobukiBridge_VERSION_CANDIDATE - The candidate version of KobukiBridge found.

message(STATUS "Found KobukiBridge-@PROJECT_VERSION@")
set(KobukiBridge_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(KobukiBridge_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(KobukiBridge_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(KobukiBridge_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(KobukiBridge_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(KobukiBridge_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(KobukiBridge_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(KobukiBridge_DEFINITIONS ${<dependency>_DEFINITIONS})

set(KobukiBridge_VERSION @PROJECT_VERSION@)
set(KobukiBridge_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(KobukiBridge_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(KobukiBridge_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(KobukiBridge_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

